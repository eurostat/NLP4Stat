<?xml version="1.0"?>
<rdf:RDF xmlns="http://nlp4statref/ontology#"
     xml:base="http://nlp4statref/ontology"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:estat="http://nlp4statref/knowledge/ontology#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#">
    <owl:Ontology rdf:about="http://nlp4statref/ontology">
        <rdfs:comment xml:lang="en">This is the version 1.0 of the ontology for the NLP4StatRef Knowledge Database.</rdfs:comment>
        <owl:versionInfo xml:lang="en">2022.04.6 - This ontology carries all the classes and properties necessary for the description of the knowledge resulting from the processed data. 
It must be enriched with the description of the enrichments resulting from semantic analyses.
This version does not integrate the standard ontologies, the association being carried by the instantiation of the Knowledge database. An update of the ontology will be done, after the control of the instantiation, integrating then standard ontologies.</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://nlp4statref/ontology#hasCODEDTerm -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasCODEDTerm">
        <rdfs:subPropertyOf rdf:resource="http://nlp4statref/ontology#hasGlossaryTerm"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://nlp4statref/ontology#Classification"/>
                    <rdf:Description rdf:about="http://nlp4statref/ontology#Content"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#CODEDTerm"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates entities from the Editorial Content with the CODED Terms</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasCODEDTerm</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasCategory -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasCategory">
        <rdfs:subPropertyOf rdf:resource="http://nlp4statref/ontology#hasClassification"/>
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Content"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#Category"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates an article from the Editorial Content with its Categories</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasCategory</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasCategoryOfGlossaryArticle -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasCategoryOfGlossaryArticle">
        <rdfs:subPropertyOf rdf:resource="http://nlp4statref/ontology#hasCategory"/>
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#GlossaryArticle"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#GlossaryArticleCategory"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates an article from the glossary category with its category(-ies)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasCategoryOfGlossaryArticle</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasCategoryOfStatisticExplainedArticle -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasCategoryOfStatisticExplainedArticle">
        <rdfs:subPropertyOf rdf:resource="http://nlp4statref/ontology#hasCategory"/>
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#StatisticsExplainedArticle"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#StatisticsExplainedCategory"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates an article from the statistics explained category with its category(-ies)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasCategoryOfStatisticExplainedArticle</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasClassification -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasClassification">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Content"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#Classification"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that shows if an article from the Editorial Content has any Classification</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasClassification</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasCode -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasCode">
        <rdfs:subPropertyOf rdf:resource="http://nlp4statref/ontology#hasGlossaryTerm"/>
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#StatisticalData"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#Code"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates a dataset to its code(-s). Basically is the labels of the Statistical Data</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasCode</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasGlossaryTerm -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasGlossaryTerm">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Content"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#GlossaryTerm"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that shows if an article from the Editorial Content has any Glossary Term</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasGlossaryTerm</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasNamedEntity -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasNamedEntity">
        <rdfs:subPropertyOf rdf:resource="http://nlp4statref/ontology#hasGlossaryTerm"/>
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Content"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#NamedEntity"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates an article from the Editorial Content with the named entities that it contains</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasNamedEntity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasOECDTerm -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasOECDTerm">
        <rdfs:subPropertyOf rdf:resource="http://nlp4statref/ontology#hasGlossaryTerm"/>
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Content"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#OECDTerm"/>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasOECDTheme -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasOECDTheme">
        <rdfs:subPropertyOf rdf:resource="http://nlp4statref/ontology#hasTheme"/>
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#OECDTerm"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#OECDTheme"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates the OECD themes with the OECD terms</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasOECDTheme</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasParagraph -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasParagraph">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#StatisticsExplainedArticle"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#Paragraph"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates the Statistics Explained Articles with its paragraphs</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasParagraph</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasReference -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasReference">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://nlp4statref/ontology#CODEDTerm"/>
                    <rdf:Description rdf:about="http://nlp4statref/ontology#Content"/>
                    <rdf:Description rdf:about="http://nlp4statref/ontology#OECDTerm"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#Reference"/>
        <rdfs:comment>A property that relates an article or a dataset with its resources</rdfs:comment>
        <rdfs:label>hasResource</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasTheme -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasTheme">
        <rdfs:subPropertyOf rdf:resource="http://nlp4statref/ontology#hasClassification"/>
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Content"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#Theme"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasTheme</rdfs:label>
        <owl:backwardCompatibleWith rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates the Editorial Content entities with their Theme(-s)</owl:backwardCompatibleWith>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasTopic -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasTopic">
        <rdfs:subPropertyOf rdf:resource="http://nlp4statref/ontology#hasClassification"/>
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Content"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#Topic"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates an article from the Editorial Content with its Topics</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasTopic</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#hasType -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#hasType">
        <rdfs:subPropertyOf rdf:resource="http://nlp4statref/ontology#hasClassification"/>
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Content"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#Type"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Some articles from the Editorial Content may have some information about their lexical type, status type, and others</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">hasType</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#internalURI -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#internalURI">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Reference"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#Content"/>
        <rdfs:comment>A property that relates a resources with an URI from the content class</rdfs:comment>
        <rdfs:label>hasURI</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://nlp4statref/ontology#mapsToTheme -->

    <owl:ObjectProperty rdf:about="http://nlp4statref/ontology#mapsToTheme">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#OECDTheme"/>
        <rdfs:range rdf:resource="http://nlp4statref/ontology#EurostatTheme"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates a OECD Entity with the EuroStat Theme(-s)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">MapsToTheme</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://nlp4statref/ontology#content -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#content">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Article"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This property relates the articles from the editorial content with their Content (i.e., their Abstract or a small Description)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">content</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#context -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#context">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://nlp4statref/ontology#Article"/>
                    <rdf:Description rdf:about="http://nlp4statref/ontology#OECDTerm"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates the Articles from the Editorial Content with their context (a small description of the context that the article is applicable to)</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">context</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#dataSource -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#dataSource">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Article"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that connects the articles from the Editorial Content with their Data Source(-s)</rdfs:comment>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dataSource</rdfs:seeAlso>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#dateCreated -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#dateCreated">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://nlp4statref/ontology#Content"/>
                    <rdf:Description rdf:about="http://nlp4statref/ontology#GlossaryTerm"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates a glossary entity with its creation date</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dateCreated</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#dateUpdated -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#dateUpdated">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://nlp4statref/ontology#Content"/>
                    <rdf:Description rdf:about="http://nlp4statref/ontology#GlossaryTerm"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTimeStamp"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates a glossary entity with the date that was updated</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">dateUpdated</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#definition -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#definition">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://nlp4statref/ontology#CODEDTerm"/>
                    <rdf:Description rdf:about="http://nlp4statref/ontology#OECDTerm"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates a glossary entity with its context</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">definition</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#externalURL -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#externalURL">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Reference"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that indicates the URL of the Reference</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">url</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#fileDescription -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#fileDescription">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#StatisticalData"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates a dataset with its file description</rdfs:label>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">fileDescription</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#id -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#id">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://nlp4statref/ontology#Content"/>
                    <rdf:Description rdf:about="http://nlp4statref/ontology#GlossaryTerm"/>
                    <rdf:Description rdf:about="http://nlp4statref/ontology#StatisticalData"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that indicates the ID of the glossary or the content entity</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">id</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#keyword -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#keyword">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Topic"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates the an instance from the class Topic with its keyword(-s)</rdfs:label>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">keyword</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#level -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#level">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#StatisticalData"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that indicates the depth that the dataset is in the Statistical Data tree</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">level</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#linkID -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#linkID">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Reference"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that indicates the ID of the link. The ID is given by EuroStat</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">linkID</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#paragraph -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#paragraph">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#Paragraph"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates a paragraph from the paragraph class with its context</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">paragraph</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#remark -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#remark">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#CODEDTerm"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates a glossary entity with its remark</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">remark</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#sourcePublication -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#sourcePublication">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#OECDTerm"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that relates the OECD enities with their source Publication</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sourcePublication</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://nlp4statref/ontology#term -->

    <owl:DatatypeProperty rdf:about="http://nlp4statref/ontology#term">
        <rdfs:domain rdf:resource="http://nlp4statref/ontology#GlossaryTerm"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A property that indicates the label of the dataset and the label of the Vocabulary entity</rdfs:comment>

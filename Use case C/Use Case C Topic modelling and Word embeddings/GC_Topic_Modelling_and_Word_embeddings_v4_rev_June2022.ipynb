        "\n",
        "all_df['raw content']=all_df['raw content'].apply(lambda x: lemmatize_text(x))\n",
        "\n",
        "texts = all_df['raw content'].to_list()\n",
        "\n",
        "print('\\nFirst text: \\n',texts[:1])\n",
        "print('\\nTotal texts: ',len(texts),'\\n')"
